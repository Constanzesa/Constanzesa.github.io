---
layout: layouts/base.njk
title: ME - Home
---

<h2>Posts</h2>
<ul class="posts">
{% for post in collections.posts %}
  <li>
    <a href="{{ post.url }}">{{ post.data.title }}</a>
    <span class="post-date">{{ post.date | date("yyyy-MM-dd") }}</span>
    <p class="post-description">{{ post.data.description }}</p>
  </li>
{% endfor %}
</ul>
